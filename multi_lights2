import RPi.GPIO as GPIO
import time
GPIO.setmode(GPIO.BCM)
GPIO.setwarnings(False)
GPIO.setup(18,GPIO.OUT)
print('Press Ctrl-C to quit.')
try:
    while True:
        print "led on"
        GPIO.output(18,GPIO.HIGH)
        time.sleep(3)
        print "led off"
        GPIO.output(18,GPIO.LOW)
        time.sleep(2)
        print "led on"
        GPIO.output(18,GPIO.HIGH)
        time.sleep(3)
        print "led off"
        GPIO.output(18,GPIO.LOW)
        time.sleep(2)
except KeyboardInterrupt:
    print('\nDone.')c
